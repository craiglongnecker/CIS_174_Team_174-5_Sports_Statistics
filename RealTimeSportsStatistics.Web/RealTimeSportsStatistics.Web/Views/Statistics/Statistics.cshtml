@model dynamic

	<h1>Game Statistics</h1>

	<div class="table-responsive">
		<table class="table table-bordered">
				<tr>
					<th>Team 1 ID</th>
					<th>Team 1 Name</th>
					<th>Team 1 Score</th>
					<th>Team 1 First Downs</th>
					<th>Team 1 Total Yards</th>
					<th>Team 1 Rush Attempts</th>
					<th>Team 1 Total Rushing Yards</th>
					<th>Team 1 Passes Completed</th>
					<th>Team 1 Passes Attempted</th>
					<th>Team 1 Passes Intercepted</th>
					<th>Team 1 Total Passing Yards</th>
					<th>Team 1 Fumbles</th>
					<th>Team 1 Fumbles Lost</th>
					<th>Team 1 Number of Sacks</th>
					<th>Team 1 Total Sack Yards Lost</th>
					<th>Team 1 Number of Punts</th>
					<th>Team 1 Average Yards Per Punt</th>
					<th>Team 1 Number Of Penalties</th>
					<th>Team 1 Total Penalty Yards</th>
				</tr>
				@foreach (var statistics in Model.Statistics)
				{
					<tr>
						<td>
							@statistics.Team1Id
						</td>
						<td>
							@statistics.Team
						</td>
						<td>
							@statistics.Team1TotalScore
						</td>
						<td>
							@statistics.Team1TotalFirstDowns
						</td>
						<td>
							@statistics.Team1TotalYards
						</td>
						<td>
							@statistics.Team1TotalRushes
						</td>
						<td>
							@statistics.Team1TotalRushingYards
						</td>
						<td>
							@statistics.Team1TotalPassesCompleted
						</td>
						<td>
							@statistics.Team1TotalPassesAttempted
						</td>
						<td>
							@statistics.Team1TotalPassesIntercepted
						</td>
						<td>
							@statistics.Team1TotalPassingYards
						</td>
						<td>
							@statistics.Team1TotalFumbles
						</td>
						<td>
							@statistics.Team1TotalFumblesLost
						</td>
						<td>
							@statistics.Team1TotalSacks
						</td>
						<td>
							@statistics.Team1TotalSackYardsLost
						</td>
						<td>
							@statistics.Team1TotalNumberOfPunts
						</td>
						<td>
							@statistics.Team1AverageYardsPerPunt
						</td>
						<td>
							@statistics.Team1TotalNumberOfPenalties
						</td>
						<td>
							@statistics.Team1TotalPenaltyYards
						</td>
					</tr>
				}
			</table>
		<table class="table table-bordered">
			<tr>
				<th>Team 2 ID</th>
				<th>Team 2 Name</th>
				<th>Team 2 Score</th>
				<th>Team 2 First Downs</th>
				<th>Team 2 Total Yards</th>
				<th>Team 2 Rushing Attempts</th>
				<th>Team 2 Total Rushing Yards</th>
				<th>Team 2 Passes Completed</th>
				<th>Team 2 Passes Attempted</th>
				<th>Team 2 Passes Intercepted</th>
				<th>Team 2 Total Passing Yards</th>
				<th>Team 2 Fumbles</th>
				<th>Team 2 Fumbles Lost</th>
				<th>Team 2 Number of Sacks</th>
				<th>Team 2 Total Sack Yards Lost</th>
				<th>Team 2 Number of Punts</th>
				<th>Team 2 Average Yards Per Punt</th>
				<th>Team 2 Number Of Penalties</th>
				<th>Team 2 Total Penalty Yards</th>
			</tr>
			@foreach (var statistics1s in Model.Statistics1s)
			{
			<tr>
				<td>
					@statistics1s.Team2Id
				</td>
				<td>
					@statistics1s.Team
				</td>

				<td>
					@statistics1s.Team2TotalScore
				</td>
				<td>
					@statistics1s.Team2TotalFirstDowns
				</td>
				<td>
					@statistics1s.Team2TotalYards
				</td>
				<td>
					@statistics1s.Team2TotalRushes
				</td>
				<td>
					@statistics1s.Team2TotalRushingYards
				</td>
				<td>
					@statistics1s.Team2TotalPassesCompleted
				</td>
				<td>
					@statistics1s.Team2TotalPassesAttempted
				</td>
				<td>
					@statistics1s.Team2TotalPassesIntercepted
				</td>
				<td>
					@statistics1s.Team2TotalPassingYards
				</td>
				<td>
					@statistics1s.Team2TotalFumbles
				</td>
				<td>
					@statistics1s.Team2TotalFumblesLost
				</td>
				<td>
					@statistics1s.Team2TotalSacks
				</td>
				<td>
					@statistics1s.Team2TotalSackYardsLost
				</td>
				<td>
					@statistics1s.Team2TotalNumberOfPunts
				</td>
				<td>
					@statistics1s.Team2AverageYardsPerPunt
				</td>
				<td>
					@statistics1s.Team2TotalNumberOfPenalties
				</td>
				<td>
					@statistics1s.Team2TotalPenaltyYards
				</td>
			</tr>
			}
		</table>
	</div>

